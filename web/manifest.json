{
    "manifest_version": 2,
    "version": "0.0.1",
    "name": "yowl",
    "short_name": "yowl",
    "description": "Yowl l'extension qui permet de tout commenter",
    "browser_action": {
        "default_popup": "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "icons": {
        "256": "icons/owl256.png",
        "512": "icons/owl512.png"
    }
}
